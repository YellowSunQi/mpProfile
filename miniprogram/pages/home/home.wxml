<!--miniprogram/pages/home/home.wxml-->
<view class='container'>
   <view class='banner'>
      <swiper 
		indicator-dots="{{indicatorDots}}"
		autoplay="{{autoPlay}}"
		duration="{{duration}}"
		interval='{{interval}}'
		circular='true'
		>
		<block wx:for="{{data.bannerList}}" wx:key="{{key}}">
         <swiper-item>
            <image src="{{item}}" class="slide-image" />
         </swiper-item>
		</block>	
      </swiper>
   </view>
</view>